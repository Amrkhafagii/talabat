{
  "expo": {
    "name": "bolt-expo-nativewind",
    "slug": "setbet",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/icon.png",
    "scheme": "myapp",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "splash": {
      "image": "./assets/images/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "ios": {
      "bundleIdentifier": "com.yourcompany.app",
      "supportsTablet": true
    },
    "android": {
      "package": "com.yourcompany.app",
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/adaptive-icon.png",
        "backgroundColor": "#FFFFFF"
      }
    },
    "web": {
      "bundler": "metro",
      "output": "single",
      "favicon": "./assets/images/favicon.png"
    },
    "notification": {
      "vapidPublicKey": "YOUR_VAPID_KEY"
    },
    "extra": {
      "eas": {
        "projectId": "fc7cc518-e76e-4c7d-82b1-ceca8061336f"
      }
    },
    "plugins": [
      "expo-router",
      "expo-font",
      "expo-web-browser",
      "expo-notifications"
    ],
    "experiments": {
      "typedRoutes": true
    },
    "owner": "amrkhafagi"
  }
}
