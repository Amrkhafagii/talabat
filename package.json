{
  "name": "bolt-expo-starter",
  "main": "expo-router/entry",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "dev": "EXPO_NO_TELEMETRY=1 expo start",
    "build:web": "expo export --platform web",
    "lint": "expo lint",
    "test:contract": "node --test tests/contract/rpcs.test.mjs",
    "test:smoke": "vitest run tests/smoke/admin.smoke.test.tsx",
    "test": "npm run test:contract && npm run test:smoke",
    "check:env": "node scripts/check-env.js",
    "diagnose": "npm run check:env && npm run lint && tsc --noEmit --skipLibCheck && npm run test:smoke",
    "fix:lint": "eslint \"app/**/*.{ts,tsx}\" \"components/**/*.{ts,tsx}\" --fix --plugin unused-imports --rule \"unused-imports/no-unused-imports:error\" --rule \"unused-imports/no-unused-vars:off\"",
    "typecheck": "tsc --noEmit --skipLibCheck",
    "solvetypecheck": "npm run typecheck -- --pretty false > typecheck.log"
  },
  "dependencies": {
    "@expo-google-fonts/inter": "^0.2.3",
    "@expo/vector-icons": "^14.1.0",
    "@hookform/resolvers": "^3.3.4",
    "@lucide/lab": "^0.1.2",
    "@react-navigation/bottom-tabs": "^7.2.0",
    "@react-navigation/native": "^7.0.14",
    "expo-clipboard": "~6.0.2",
    "@supabase/supabase-js": "^2.39.0",
    "@react-native-async-storage/async-storage": "^1.23.1",
    "expo": "^53.0.0",
    "expo-blur": "~14.1.5",
    "expo-camera": "~16.1.9",
    "expo-constants": "~17.1.6",
    "expo-document-picker": "^14.0.7",
    "expo-font": "~13.3.1",
    "expo-haptics": "~14.1.4",
    "expo-linear-gradient": "~14.1.5",
    "expo-linking": "~7.1.5",
    "expo-location": "~18.1.1",
    "expo-notifications": "^0.32.13",
    "expo-router": "~5.1.1",
    "expo-splash-screen": "~0.30.9",
    "expo-status-bar": "~2.2.3",
    "expo-symbols": "~0.4.5",
    "expo-system-ui": "~5.0.9",
    "expo-web-browser": "~14.2.0",
    "lucide-react-native": "^0.475.0",
    "react": "19.0.0",
    "react-dom": "19.0.0",
    "react-hook-form": "^7.49.3",
    "react-native": "0.79.4",
    "react-native-gesture-handler": "~2.24.0",
    "react-native-reanimated": "~3.17.4",
    "react-native-safe-area-context": "5.4.0",
    "react-native-screens": "~4.11.1",
    "react-native-svg": "15.11.2",
    "react-native-url-polyfill": "^2.0.0",
    "react-native-web": "^0.20.0",
    "react-native-webview": "13.13.5",
    "zod": "^3.22.4"
  },
  "devDependencies": {
    "@babel/core": "^7.25.2",
    "@types/react": "~19.0.10",
    "eslint": "^9.39.1",
    "eslint-config-expo": "~9.2.0",
    "knip": "^5.61.3",
    "typescript": "~5.8.3",
    "@playwright/test": "^1.48.2",
    "vitest": "^2.1.4",
    "@testing-library/react-native": "^12.7.0",
    "@testing-library/jest-native": "^5.4.3",
    "react-test-renderer": "19.0.0",
    "eslint-plugin-unused-imports": "^3.2.0"
  }
}
